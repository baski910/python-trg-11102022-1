   "source": [
    "# pivot table example\n",
    "\n",
    "import pandas as pd\n",
    "\n",
    "\n",
    "df = pd.read_excel('sales.xlsx')\n",
    "\n",
    "print(pd.pivot_table(df,index=[\"Manager\",\"Rep\"],values=\"Price\",aggfunc=sum))"
   ]
  },
